{
  "version": 2,
  "rewrites": [
    {
      "source": "/campaigns/dmk-2016",
      "destination": "/campaigns/dmk-2016.html"
    },
    {
      "source": "/campaigns/iuml-2019",
      "destination": "/campaigns/iuml-2019.html"
    },
    {
      "source": "/campaigns/iuml-2021",
      "destination": "/campaigns/iuml-2021.html"
    },
    {
      "source": "/campaigns/inc-2023",
      "destination": "/campaigns/inc-2023.html"
    },
    {
      "source": "/campaigns/iuml-ijk-2024",
      "destination": "/campaigns/iuml-ijk-2024.html"
    }
  ],
  "redirects": [
    {
      "source": "/campaigns/dmk-2016.html",
      "destination": "/campaigns/dmk-2016",
      "permanent": true
    },
    {
      "source": "/campaigns/iuml-2019.html",
      "destination": "/campaigns/iuml-2019",
      "permanent": true
    },
    {
      "source": "/campaigns/iuml-2021.html",
      "destination": "/campaigns/iuml-2021",
      "permanent": true
    },
    {
      "source": "/campaigns/inc-2023.html",
      "destination": "/campaigns/inc-2023",
      "permanent": true
    },
    {
      "source": "/campaigns/iuml-ijk-2024.html",
      "destination": "/campaigns/iuml-ijk-2024",
      "permanent": true
    }
  ],
  "headers": [
    {
      "source": "/src/styles/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/Campaign Images/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    }
  ]
} 