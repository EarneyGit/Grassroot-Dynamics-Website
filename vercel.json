{
  "version": 2,
  "routes": [
    {
      "src": "/campaigns/dmk-2016",
      "dest": "/campaigns/dmk-2016.html"
    },
    {
      "src": "/campaigns/iuml-2019",
      "dest": "/campaigns/iuml-2019.html"
    },
    {
      "src": "/campaigns/iuml-2021",
      "dest": "/campaigns/iuml-2021.html"
    },
    {
      "src": "/campaigns/inc-2023",
      "dest": "/campaigns/inc-2023.html"
    },
    {
      "src": "/campaigns/iuml-ijk-2024",
      "dest": "/campaigns/iuml-ijk-2024.html"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "headers": [
    {
      "source": "/(.*\\.(css|js|png|jpg|jpeg|gif|svg|ico))",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    }
  ]
} 